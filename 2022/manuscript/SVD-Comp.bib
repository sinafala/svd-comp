	Id = {2330809382667},
	Isbn = {1549-1277},
	Journal = {PLoS medicine},
	La = {English},
	Number = {8},
	Title = {Child mortality estimation: appropriate time periods for child mortality estimates from full birth histories.},
	Ty = {JOUR},
	Volume = {9},
	Year = {2012}}

